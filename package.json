{
  "name": "office-print",
  "description": "一款用于在网页中打印Office文件的软件，可以打印包括Word、PowerPoint、Excel、PDF、图片等文件。",
  "version": "1.1.0",
  "main": "index.mjs",
  "author": "innben <hinjin@innben.com> (https://www.innben.com/print)",
  "bugs": {
    "url": "https://github.com/a6051529/office-print/issues",
    "email": "hinjin@innben.com"
  },
  "contributors": [
    "innben <hinjin@innben.com> (https://www.innben.com/print)"
  ],
  "dependencies": {
    "ava": "^6.0.1"
  },
  "files": [
    "index.mjs",
    "index.umd.js",
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "^6.0.1",
    "codecov": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-xo": "latest",
    "eslint-plugin-ava": "latest",
    "eslint-plugin-compat": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-unicorn": "latest",
    "fixpack": "latest",
    "husky": "latest",
    "javascript-obfuscator": "^4.1.0",
    "lint-staged": "latest",
    "nyc": "latest",
    "prettier": "latest",
    "remark-cli": "^9.0.0",
    "remark-preset-github": "latest",
    "rollup-obfuscator": "^4.1.1",
    "rollup-plugin-uglify": "^6.0.4",
    "vite": "^4.3.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "homepage": "https://github.com/a6051529/office-print",
  "keywords": [
    "print",
    "office print",
    "excel print",
    "word print",
    "powerpoint print",
    "pdf print",
    "image print"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/a6051529/office-print"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "vite build && tsc -p tsconfig.json",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc npm run test"
  }
}
